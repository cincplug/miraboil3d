{% extends '../layout/default.njk' %}

{% block body %}
  <div class="grid">
    {% component 'header' %}
    <main class="main-wrap grid-wrap">
      {% for scene in scenes %}
        <div class="column">
          {% component 'scene', {
            options: scene
          } %}
        </div>
      {% endfor %}
    </main>
  </div>
{% endblock %}
