<div class="january" data-module="january/January" data-options="{{ options | dump }}">
  <h3 class="january__title">{{ title }}</h3>
  <button class="january__color-switch" data-color="red"></button>
  <button class="january__color-switch" data-color="green"></button>
  <button class="january__color-switch" data-color="blue"></button>
  <button class="january__color-switch" data-color="yellow"></button>
  <div class="january__canvas"></div>
</div>
